{
  "compilerOptions": {
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "forceConsistentCasingInFileNames": true,
    "target": "es5",
    "isolatedModules": false,
    "module": "es2020",
    "lib": [
      "esnext",
      "dom"
    ],
 },
 "exclude": [
    "node_modules",
    "./test/*.spec.ts"
  ]
} 
